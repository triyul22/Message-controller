@{
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta ="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="2" cellspacing="2" border="1">
        <tr>
            <th>Id</th>
            <th>Text</th>
            <th>SentTime</th>
        </tr>
        @foreach (var message in ViewBag.messageList)
        {
            <tr>
                <td>@message.Id</td>
                <td>@message.Text</td>
                <td>@message.SentTime</td>
            </tr>
        }
    </table>
</body>
</html>

